    "4. Create a Bokeh histogram to visualize the distribution of the given data.\n",
    "        data_hist = np.random.randn(1000) ",
    "\n",
    "        hist, edges = np.histogram(data_hist, bins=30)\n",
    "Ans.\n",
    "        import numpy as np\n",
    "        from bokeh.plotting import figure, show\n",
    "        from bokeh.io import output_notebook\n",
    "        \n",
    "        # Output in the notebook (if using Jupyter)\n",
    "        output_notebook()\n",
    "        \n",
    "        # Generate random data\n",
    "        data_hist = np.random.randn(1000)\n",
    "        \n",
    "        # Calculate the histogram\n",
    "        hist, edges = np.histogram(data_hist, bins=30)\n",
    "        \n",
    "        # Create a Bokeh figure\n",
    "        p = figure(title=\"Histogram of Normally Distributed Data\",\n",
    "                   x_axis_label='Value',\n",
    "                   y_axis_label='Count',\n",
    "                   width=800,\n",
    "                   height=400)\n",
    "        \n",
    "        # Add a quad glyph for the histogram\n",
    "        p.quad(top=hist, bottom=0, left=edges[:-1], right=edges[1:], fill_color='skyblue', line_color='black')\n",
    "        \n",
    "        # Show the plot\n",
    "        show(p)\n"
   ]
